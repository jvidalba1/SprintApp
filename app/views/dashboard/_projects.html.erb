<% context.instance_eval do |attr_table|
attr_table.column("Nombre") { |project| link_to( project.display_name, project_path(project) ) }
attr_table.column("Cliente") { |project| link_to( project.client_name, client_path(project.client) ) }
attr_table.column("Autor") { |project| modified_by(project) }
end %>